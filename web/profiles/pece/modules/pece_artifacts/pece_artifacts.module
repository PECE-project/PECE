<?php
/**
 * @file
 * Code for the PECE Artifacts.
 */

/**
 * Implements HOOK_module_implements_alter
 *
 * Prevent the licenses_vocabulary module from creating license terms on install
 *
 * @param array $implementations
 * @param string $hook
 * @return void
 */
 function pece_artifacts_module_implements_alter(&$implementations, $hook) {
  if ($hook === 'modules_installed') {
      unset($implementations['licenses_vocabulary']);
  }
}
