{
    "name": "pece-project/drupal-pece",
    "description": "PECE Drupal distribution",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal/empty_page": {
            "type": "git",
            "url": "git@git.drupal.org:issue/empty_page-3287186.git"
        },
        "drupal/location_migration": {
            "type": "git",
            "url": "git@git.drupal.org:issue/location_migration-3288357.git"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^2",
        "cweagans/composer-patches": "^1",
        "drupal/access_policy": "^1.0@beta",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^3.3",
        "drupal/advanced_text_formatter": "^3",
        "drupal/asset_injector": "^2.20",
        "drupal/better_exposed_filters": "^6",
        "drupal/bibcite": "^3",
        "drupal/bibcite_migrate": "^2.0@beta",
        "drupal/blazy": "^3",
        "drupal/block_class": "^2.0",
        "drupal/blocktabs": "^1.0@beta",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_layout_builder": "^2.0",
        "drupal/bricks": "^2.0",
        "drupal/bulma": "3.0.x-dev@dev",
        "drupal/cer": "^5",
        "drupal/color_field": "^3",
        "drupal/components": "^3",
        "drupal/config_filter": "^2.2",
        "drupal/config_partial_export": "^2",
        "drupal/config_split": "^2.0.0-beta4",
        "drupal/config_sync": "^3",
        "drupal/config_update": "^2",
        "drupal/configurable_views_filter_block": "^1.0@beta",
        "drupal/context": "^5",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^3.11-rc1",
        "drupal/dashboards": "^2.1@beta",
        "drupal/default_content": "^2.0@alpha",
        "drupal/eck": "^2",
        "drupal/elasticsearch_connector": "^7.0@alpha",
        "drupal/empty_page": "8.x-1.x-dev",
        "drupal/facets": "^3.0@beta",
        "drupal/field_formatter_range": "^1.4",
        "drupal/field_group": "^3.0",
        "drupal/foundation_sites": "dev-2.x@dev",
        "drupal/geocoder": "^4",
        "drupal/geofield": "^1.3",
        "drupal/geolocation": "^3.2",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_login": "^1.3",
        "drupal/gin_type_tray": "^1.0@beta",
        "drupal/graphql": "4.x-dev",
        "drupal/image_replace": "^2.0",
        "drupal/inline_entity_form": "^1.0-rc8",
        "drupal/languagefield": "1.x-dev",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_builder_operation_link": "^2.1",
        "drupal/layout_library": "^1.0@beta",
        "drupal/layout_paragraphs": "^2.0",
        "drupal/lb_tabs": "3.0.x-dev",
        "drupal/lbl": "^1.0",
        "drupal/licenses_vocabulary": "^2.0@alpha",
        "drupal/location_migration": "1.0.x-dev",
        "drupal/m4032404": "^1.0@alpha",
        "drupal/matomo": "^1.11",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_library_media_modify": "^1.0@beta",
        "drupal/menu_block": "^1.10",
        "drupal/metatag": "^1.10",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6",
        "drupal/paragraphs": "^1.9",
        "drupal/pathauto": "^1",
        "drupal/pbf": "^1.0",
        "drupal/piwik_noscript": "^1.5",
        "drupal/profile": "1.x-dev@dev",
        "drupal/radix": "^4.10",
        "drupal/redirect": "^1.6",
        "drupal/registration_role": "^2.0",
        "drupal/restui": "^1.17",
        "drupal/rules": "3.x-dev",
        "drupal/search_api": "^1.31",
        "drupal/simple_oauth": "^5.0",
        "drupal/single_content_sync": "^1.4",
        "drupal/smart_trim": "^2.0",
        "drupal/splide": "^2.0",
        "drupal/style_guide": "^1.0@dev",
        "drupal/tagclouds": "^2.0",
        "drupal/text_field_formatter": "^2.0",
        "drupal/ui_patterns": "^1.2",
        "drupal/ui_patterns_field_formatters": "^1.7",
        "drupal/ui_patterns_layout_builder": "^1.1",
        "drupal/user_restrictions": "^2.0",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/workflow": "^1.4",
        "drush/drush": "*",
        "npm-asset/blazy": "^1.8",
        "npm-asset/splidejs--splide": "^4.1",
        "oomphinc/composer-installers-extender": "~1.0 || ~2.0"
    },
    "require-dev": {
        "codeception/codeception": "^5",
        "codeception/module-asserts": "^2.0",
        "drupal/config_devel": "^1.8",
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.0",
        "drupal/drupal-driver": "^2.1.1",
        "drupal/migrate_devel": "^2",
        "ec-europa/toolkit": "^9",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "scripts": {
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/jquery-ui-touch-punch": [
                "bower-asset/jqueryui-touch-punch"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": [
                {
                    "name": "objectfitpolyfill",
                    "package": "bower-asset/objectfitpolyfill"
                },
                {
                    "name": "chartjs",
                    "package": "bower-asset/chartjs"
                },
                {
                    "name": "c3",
                    "package": "bower-asset/c3"
                },
                {
                    "name": "d3",
                    "package": "bower-asset/d3"
                },
                {
                    "name": "dropzone",
                    "package": "npm-asset/dropzone"
                },
                {
                    "name": "blazy",
                    "package": "npm-asset/blazy"
                },
                {
                    "name": "slick",
                    "package": "npm-asset/slick-carousel"
                },
                {
                    "name": "splide",
                    "package": "npm-asset/splide"
                },
                {
                    "name": "ace",
                    "package": "npm-asset/ace-builds"
                },
                {
                    "name": "jquery-ui-touch-punch",
                    "package": "bower-asset/jqueryui-touch-punch"
                },
                {
                    "name": "imagesloaded",
                    "package": "npm-asset/imagesloaded"
                },
                {
                    "name": "swagger-ui",
                    "package": "npm-asset/swagger-ui-dist"
                },
                {
                    "name": "spectrum-colorpicker",
                    "package": "npm-asset/spectrum"
                }
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "allowed-packages": [
                "drupal/core",
                "drupal/ui_patterns_layout_builder"
            ],
            "file-mapping": {
                "[web-root]/profiles/README.txt": false,
                "[web-root]/profiles/.gitignore": false
            },
            "locations": {
                "web-root": "./web"
            }
        },
        "patches": {
            "drupal/core": {
                "Missing required data for configuration: x": "https://www.drupal.org/files/issues/2023-04-06/3304778-25.patch",
                "MenuLinkContent menu_name max length is too long": "https://www.drupal.org/files/issues/2024-02-23/3087632-48.patch"
            },
            "drupal/eck": {
                "2815453 - Add migrations for Drupal 7 ECK to Drupal 8/9": "https://www.drupal.org/files/issues/2020-11-13/eck-migrate-d9-2815453-36.patch"
            },
            "drupal/paragraphs": {
                "Error: Call to a member function getEnabledBehaviorPlugins() on null in Drupal paragraphs ParagraphViewBuilder->buildMultiple()": "https://www.drupal.org/files/issues/2021-05-06/paragraphs-2895561-32.patch"
            }
        }
    }
}
