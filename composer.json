        "drupal/coder": "8.3.13",
        "drupal/drupal-driver": "2.1.1",
        "drupal/drupal-extension": "~4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": "@toolkit-install",
        "post-update-cmd": "@toolkit-install",
        "toolkit-install": "PROJECT=$(pwd) composer run-script toolkit-install -d ./vendor/ec-europa/toolkit"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        },
        {
          "type": "package",
          "package": {
            "name": "drupal/kw_itemnames",
            "type": "drupal-module",
            "version": "1.0",
            "source": {
              "url": "https://github.com/kraftwagen/kw-itemnames",
              "type": "git",
              "reference": "master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "drupal/kw_manifests",
            "type": "drupal-module",
            "version": "1.0",
            "source": {
              "url": "https://github.com/kraftwagen/kw-manifests",
              "type": "git",
              "reference": "master"
            }
          }
        },
        {
            "type": "path",
            "url": "./src/*/*"
        }
    ],
    "extra": {
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
              "web-root": "./build"
            }
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "build": [
                "type:drupal-core"
            ],
            "build/profiles/pece/libraries/{$name}": [
                "type:drupal-library"
            ],
            "build/profiles/pece/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "build/profiles/pece/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "build/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "build/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "build/profiles/pece/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "build/profiles/pece/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SetupWizard.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEuropa\\Site\\Tests\\": "./tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
